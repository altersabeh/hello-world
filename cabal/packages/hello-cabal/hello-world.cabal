cabal-version:   3.4
name:            hello-world
version:         1.0.0.0
build-type:      Simple

source-repository head
    type:     git
    location: https://github.com/altersabeh/hello-world

executable hello-cabal
    main-is:          Main.hs
    hs-source-dirs:   app
    other-modules:    Paths_hello_world
    autogen-modules:  Paths_hello_world
    default-language: GHC2021
    ghc-options:      -Wall
    build-depends:
        base ^>=4.18.2.0,
        cabal-hello
